{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReportsConfiguration": {
    "BasePath": "..\\reports",
    "TemporaryDirectory": "C:\\temp\\"
  },
  "TemporaryFileCache": {
    "CacheDirectory": "temp_downloads",
    "ExpirationTimeMinutes": 60,
    "CleanupIntervalMinutes": 30,
    "MaxCacheSizeGB": 5,
    "EnableMD5Validation": true
  }
}
